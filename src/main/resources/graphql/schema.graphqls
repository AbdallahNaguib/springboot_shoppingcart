type Query {
    categories:[Category]!
    categoryById(id:ID!):Category
}
type Mutation {
    addProduct(product: ProductInput):ID
}

input ProductInput{
    name: String!
}
type Category {
    id: ID!
    name: String!
    products: [Product]
}
type Product {
    id: ID!
    name: String!
    image: String!
    description:String!
    price:Int!
    quantity:Int
}